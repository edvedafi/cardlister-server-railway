{
  "name": "cardlister",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "engines": {
    "node": "22"
  },
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "seed:once": "node script/seedOnce.js",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env npm run build:server && medusa develop",
    "migrate": "cross-env medusa migrations run",
    "release:link": "railway link",
    "redis": "cross-env redis-stack-server",
    "deploy": "cd .. && railway up",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.16",
    "@medusajs/cache-inmemory": "1.8.11",
    "@medusajs/cache-redis": "1.9.2",
    "@medusajs/event-bus-local": "1.9.9",
    "@medusajs/event-bus-redis": "1.8.14",
    "@medusajs/file-local": "1.0.4",
    "@medusajs/inventory": "1.11.7",
    "@medusajs/medusa": "1.20.9",
    "@medusajs/stock-location": "1.11.6",
    "@tanstack/react-query": "5.53.1",
    "ajv": "8.17.1",
    "body-parser": "1.20.2",
    "bullmq": "5.4.2",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "ebay-api": "9.1.1",
    "express": "4.19.2",
    "jszip": "3.10.1",
    "medusa-file-cloudinary": "1.0.2",
    "medusa-fulfillment-manual": "1.1.41",
    "medusa-interfaces": "1.3.10",
    "medusa-payment-manual": "1.0.25",
    "medusa-payment-stripe": "6.0.11",
    "patch-package": "8.0.0",
    "postinstall-postinstall": "2.1.0",
    "prism-react-renderer": "2.4.0",
    "puppeteer": "^23.3.0",
    "typeorm": "0.3.20",
    "webdriverio": "9.0.7"
  },
  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "7.25.2",
    "@babel/preset-typescript": "7.24.7",
    "@eslint/js": "9.9.1",
    "@medusajs/medusa-cli": "1.3.23",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.1",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "babel-preset-medusa-package": "1.1.19",
    "cross-env": "7.0.3",
    "eslint": "9.9.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "@medusajs/admin-ui": {
      "@medusajs/ui-preset": "^1.1.2"
    }
  },
  "resolutions": {
    "@medusajs/ui-preset": "^1.1.2",
    "string-width": "4.2.3",
    "bullmq": "5.4.2"
  }
}
